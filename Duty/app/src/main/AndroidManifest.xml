<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jun.duty">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"
            android:label="Relative Layout">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LinearActivity"
            android:label="LayOut Dasar"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".SpinnerActivity"
            android:label="Model Spinner"
            android:parentActivityName=".LinearActivity" />
        <activity
            android:name=".RadioButtonActivity"
            android:label="Radio Button"
            android:parentActivityName=".LinearActivity" />
        <activity
            android:name=".CheckBoxActivity"
            android:label="Check Box"
            android:parentActivityName=".LinearActivity" />
        <activity
            android:name=".ToggleButtonActivity"
            android:label="Toggle Button"
            android:parentActivityName=".LinearActivity" />
        <activity
            android:name=".EditTextActivity"
            android:label="Edit Text Dan Kawan Kawan"
            android:parentActivityName=".LinearActivity" />
        <activity android:name=".DateActivity"
            android:label="Date Picker"
            android:parentActivityName=".LinearActivity"></activity>
    </application>

</manifest>